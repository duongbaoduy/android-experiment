<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.dbd.exp.android.dev"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="org.dbd.exp.android.ui.activity.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.dbd.exp.android.ui.activity.ListSensor"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.dbd.android.EXAMPLE" />
            </intent-filter>
        </activity>
        <activity android:name="aexp.sensors.Sensors" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.dbd.android.EXAMPLE" />
            </intent-filter>
        </activity>
        <activity android:name="aexp.sensors..SensorMonitor" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.dbd.android.EXAMPLE" />
            </intent-filter>
        </activity>
        <activity android:name="aexp.sensors..SensorSettings" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.dbd.android.EXAMPLE" />
            </intent-filter>
        </activity>

        <service android:name="aexp.sensors..SamplingService" />
    </application>

</manifest>